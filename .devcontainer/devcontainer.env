# Development environment variables for Photon
# These are loaded into the devcontainer on creation

# Python optimizations
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1

# Pip settings
PIP_DISABLE_PIP_VERSION_CHECK=1
PIP_NO_CACHE_DIR=1

# Development mode
DEVELOPMENT_MODE=true

# Photon specific settings
PHOTON_ENV=development
